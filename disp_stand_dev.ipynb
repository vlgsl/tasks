      "source": [
        "a = [1, 3, 5, 7, 9, 11]\n",
        "def disp(a):\n",
        "  b = sum(a)/len(a)\n",
        "  tot = 0\n",
        "  for i in range(len(a)):\n",
        "    c = (a[i] - b) ** 2\n",
        "    tot += c\n",
        "  d = tot/b\n",
        "  return d\n",
        "disp(a)"
      ],
